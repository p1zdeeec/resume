<footer>
    <div class="row">
        {{#each footer}}
        <div class="col">
            {{#each this}}
            <div class="nav">
                {{#if @last}}
                <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                {{else}}
                <a href="{{link}}" class="nav-link">{{text}}</a>
                {{/if}}
            </div>

            {{/each}}
        </div>
        {{/each}}
    </div>
</footer>