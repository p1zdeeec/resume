<div>
    {{#with main}}
    <main>

        <section>
            <div>
                {{> title data="My educations" big=true}}
            </div>
            <div>
                <ul>
                    {{#each educations}}
                    <li>{{name}}{{#unless isEnd}} (Ще в процесі) {{/unless}}</li>
                    {{/each}}
                </ul>
            </div>
            <div>
                {{> title data="My certificates" big=false}}
            </div>
            <ul>
                {{#each certificates}}
                <li>{{name}} ({{id}})</li>
                {{/each}}
            </ul>
            <div>
        </section>
    </main>
    {{/with}}
</div>