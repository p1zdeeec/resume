<main>
  <div class="col-12">
    {{#with navigation}}
    {{#each links}}

    <a class="btn btn-link col-2" href="{{href}}">{{text}}</a>
    {{/each}}
    <a class="btn btn-primary col-2" href="{{button.href}}">{{button.text}}</a>

    {{/with}}

    <hr />
    <br>

    {{#with header}}

    <h1>{{title}}</h1>
    <p>{{description}}</p>
    <a class="btn btn-primary" href="{{button.link}}">{{button.text}}</a>

    {{/with}}

    <hr />
    <br>

    {{#with city}}
    <h2>{{title}}</h2>
    <div class="row">
      {{#each cards}}
      <section class="col-5">
        <div class="card">
          {{#with picture}}
          <img class="card-img-top" src="{{src}}" alt="{{alt}}">
          {{/with}}
          <div class="card-body">
            <h5 class="card-title">{{title}}</h5>
            <p>{{description}}</p>
            <a href="{{button.href}}" class="btn btn-primary">{{button.text}}</a>
          </div>
        </div>
      </section>
      {{/each}}
    </div>
    {{/with}}

    <hr />
    <br>

    {{#with winner}}
    <h2>{{title}}</h2>
    {{#with table}}
    <table class="table">

      <thead>
        <tr>
          <th>#</th>
          <th>name</th>
          <th>surname</th>
          <th>nickname</th>
        </tr>
      </thead>

      <tbody>
        {{#each user}}
        <tr>
          <td>{{place}}</td>
          <td>{{name}}</td>
          <td>{{surname}}</td>
          <td>{{nickname}}</td>
        </tr>
        {{/each}}
      </tbody>

    </table>
    {{/with}}
    {{/with}}

    <br>
    <hr />

    {{#with gallery}}
    <h2>{{title}}</h2>
    <div class="row gx-1">
      {{#each cards}}

      <div class="card col-4">
        <div>
          <p class="card-header">
            {{name}}
          </p>
        </div>
        <ul class="list-group">
          <li class="list-group-item">{{items}}</li>
        </ul>

      </div>

      {{/each}}

    </div>


    {{/with}}

    <br>
    <hr />

    {{#with relaxion}}
    <p class="display-3">{{title}}</p>

    <div class="row gy-2">
      {{#each cards}}

      <div class="col-5">
        <div class="card">
          <div class="card-header">{{header}}</div>
          <div class="card-body">
            <h6 class="card-title">{{title}}</h6>
            <p>{{description}}</p>
          </div>
        </div>
      </div>

      {{/each}}

      <br>
      <hr />


      {{#with relaxion}}



      {{/with}}

    </div>

    {{/with}}

  </div>














</main>